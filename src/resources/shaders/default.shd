<data id="vertex-shader">
    attribute vec4 vPosition;
    uniform mat4 uModel;

    void main() {
        gl_Position = uModel * vPosition;
        }
</data>

<data id="fragment-shader">
    precision mediump float;

    void main() {
        gl_FragColor = vec4(1.0, 0.0, 0.0, 1.0);
    }
</data>
